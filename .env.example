# Kiffusion Environment Variables
# Copy this file to .env and fill in your values

# Weights & Biases API key for experiment tracking
# Get yours at: https://wandb.ai/settings
WANDB_API_KEY=your_wandb_api_key_here

# HuggingFace token for accessing gated models and pushing
# Get yours at: https://huggingface.co/settings/tokens
HF_TOKEN=your_huggingface_token_here

# HuggingFace username for pushing models
HF_USERNAME=your_username_here

# Wallhaven API key (optional, for NSFW content access)
# Get yours at: https://wallhaven.cc/settings/account
WALLHAVEN_API_KEY=your_wallhaven_api_key_here
