{
  "model_type": "lora",
  "model_family": "kolors",
  "pretrained_model_name_or_path": "Kwai-Kolors/Kolors-diffusers",
  "pretrained_vae_model_name_or_path": "Kwai-Kolors/Kolors-diffusers",
  "variant": "fp16",
  "output_dir": "output/kolors-lora",
  "data_backend_config": "config/multidatabackend_kolors.json",

  "lora_rank": 16,
  "lora_alpha": 16,
  "lora_type": "standard",

  "num_train_epochs": 3,
  "max_train_steps": 0,
  "train_batch_size": 2,
  "gradient_accumulation_steps": 4,
  "gradient_checkpointing": true,

  "learning_rate": 4e-5,
  "lr_scheduler": "cosine",
  "lr_warmup_steps": 50,
  "optimizer": "adamw_bf16",
  "mixed_precision": "bf16",
  "base_model_precision": "no_change",
  "text_encoder_1_precision": "no_change",
  "max_grad_norm": 1.0,
  "noise_offset": 0.02,

  "resolution": 1024,
  "resolution_type": "pixel",
  "minimum_image_size": 512,
  "aspect_bucket_rounding": 2,
  "caption_dropout_probability": 0.1,
  "seed": 42,

  "validation_resolution": "1024x1024",
  "validation_guidance": 3.4,
  "validation_num_inference_steps": 20,
  "validation_seed": 42,
  "validation_prompt": "spacevision, a breathtaking photograph of a nebula with vibrant colors and swirling cosmic dust",

  "checkpointing_steps": 150,
  "checkpoints_total_limit": 5,

  "push_to_hub": false,
  "report_to": "wandb",
  "tracker_project_name": "kiffusion",
  "tracker_run_name": "kolors-lora-spacevision-v4",

  "disable_benchmark": false
}
